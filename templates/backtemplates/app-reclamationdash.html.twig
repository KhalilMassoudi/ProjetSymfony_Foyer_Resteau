    <!--**********************************
        Sidebar end
    ***********************************-->

    <!--**********************************
        Content body start
    ***********************************-->
    {% block body %}
        <div class="content-body">
            <!-- row -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-3 col-sm-6 ">
                        <div class="card" onclick="window.location.href='{{ path('app_adminafficherReclamations') }}'">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">total réclamations </div>
                                    <div class="stat-digit">  <i class="fa fa-bell"></i>{{ total_reclamations }}</div>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-success w-85" role="progressbar" aria-valuenow="" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card" onclick="window.location.href='{{ path('app_afficherReclamationsrpondues') }}'">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">total réclamations répondues</div>
                                    <div class="stat-digit"> {{total_reclamations_repondues}}</div>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar"
                                         style="width: {{ percentage_repondues }}%"
                                         aria-valuenow="{{ percentage_repondues }}"
                                         aria-valuemin="0"
                                         aria-valuemax="100">
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>



                    <div class="col-lg-3 col-sm-6">
                        <div class="card">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">total feedback </div>
                                    <div class="stat-digit"> {{total_feedbacks}}</div>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar"
                                         style="width: {{percentageFeedback}}%"
                                         aria-valuenow="{{percentageFeedback}}"
                                         aria-valuemin="0"
                                         aria-valuemax="100">
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="card" onclick="window.location.href='{{ path('app_adminafficherReclamationssuspendues') }}'">
                            <div class="stat-widget-two card-body">
                                <div class="stat-content">
                                    <div class="stat-text">Total réclamations suspendues</div>
                                    <div class="stat-digit">
                                        <i class="fa fa-exclamation-circle"></i> {{ total_reclamations - total_reclamations_repondues }}
                                    </div>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-danger" role="progressbar"
                                         style="width: {{ percentageNonRepondues }}%"
                                         aria-valuenow="{{ percentageNonRepondues }}"
                                         aria-valuemin="0"
                                         aria-valuemax="100">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                </div>

            <div class="row">
                <div class="col-xl-8 col-lg-8 col-md-8">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title"></h4> <!-- Laisser vide si aucun titre n'est nécessaire -->
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-xl-12 col-lg-8">
                                    <div>
                                        <div class="container-fluid">
                                            <div class="card">
                                                <div class="card-header">
                                                    <h4 class="card-title">
                                                        <a href="{{ path('app_type_reclamation_add') }}">Vos types de réclamation</a>
                                                    </h4>
                                                </div>
                                                <div class="card-body">
                                                    <div class="current-progress">
                                                        {% for type, pourcentage in pourcentages %}
                                                            <div class="progress-content py-2">
                                                                <div class="row align-items-center">
                                                                    <div class="col-4">
                                                                        <div class="progress-text">{{ type }}</div>
                                                                    </div>
                                                                    <div class="col-8">
                                                                        <div class="current-progressbar">
                                                                            <div class="progress">
                                                                                <div class="progress-bar progress-bar-primary"
                                                                                     role="progressbar"
                                                                                     style="width: {{ pourcentage }}%;"
                                                                                     aria-valuenow="{{ pourcentage }}"
                                                                                     aria-valuemin="0"
                                                                                     aria-valuemax="100">
                                                                                    {{ pourcentage }}%
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        {% else %}
                                                            <p>Aucun type de réclamation disponible.</p>
                                                        {% endfor %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-4 col-md-4">
                    <div class="card">
                        <div class="card-body text-center">
                            <div class="m-t-10">
                                <h4 class="card-title">Customer Feedback</h4>
                                <h2 class="mt-3">{{total_feedbacks}}</h2>
                            </div>

                            <ul class="widget-line-list m-b-15">
                                <li class="border-right">
                                    {{ percentage_positive_ratings | number_format(1) }}% <br>
                                    <span class="text-success">
                            <i class="ti-hand-point-up"></i> Positive
                        </span>
                                </li>
                                <li>
                                    {{ percentage_negative_ratings | number_format(1) }}% <br>
                                    <span class="text-danger">
                            <i class="ti-hand-point-down"></i> Negative
                        </span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>


                <div class="row">
                    <div class="container-fluid">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Timeline</h4>
                            </div>
                            <div class="card-body">
                                <div class="widget-timeline">
                                    <ul class="timeline">
                                        {% for item in reclamations %}
                                            <li>
                                                <div class="timeline-badge {% if loop.index is odd %}primary{% else %}warning{% endif %}"></div>
                                                <a class="timeline-panel text-muted" href="#">
                                                    <span>{{ item.timeAgo }}</span>
                                                    <h6 class="m-t-5">{{ item.reclamation.titre }}</h6>
                                                </a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                    </div>



            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->
    {% endblock %}

    <!--**********************************
        Footer start
    ***********************************-->
    {% block footer %}
        <div class="footer">
            <div class="copyright">
                <p>Copyright © Designed &amp; Developed by <a href="#" target="_blank">Quixkit</a> 2019</p>
                <p>Distributed by <a href="https://themewagon.com/" target="_blank">Themewagon</a></p>
            </div>
        </div>
    {% endblock %}
    <!--**********************************
        Footer end
    ***********************************-->

