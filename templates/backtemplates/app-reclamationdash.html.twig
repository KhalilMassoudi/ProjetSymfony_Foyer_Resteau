                            <li><a href="{{ path ('app_categorieplat') }}">Catégorie Plat</a></li>
                            <li><a href="{{ path('app_plat') }}">Plats</a></li>
                        </ul>

                    </li>
                    <li><a class="has-arrow" href="javascript:void()" aria-expanded="false"><i
                                    class="icon icon-chart-bar-33"></i><span class="nav-text">Gestion des chambres et des equipements </span></a>
                        <ul aria-expanded="false">
